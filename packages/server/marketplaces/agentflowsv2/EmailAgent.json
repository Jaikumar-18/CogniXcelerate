          "type": "CustomFunction",
          "color": "#E4B7FF",
          "baseClasses": [
            "CustomFunction"
          ],
          "category": "Agent Flows",
          "description": "Execute custom function",
          "inputParams": [
            {
              "label": "Input Variables",
              "name": "customFunctionInputVariables",
              "description": "Input variables can be used in the function with prefix $. For example: $foo",
              "type": "array",
              "optional": true,
              "acceptVariable": true,
              "array": [
                {
                  "label": "Variable Name",
                  "name": "variableName",
                  "type": "string"
